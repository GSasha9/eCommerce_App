{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier \"**/*.{ts,scss,css}\"",
    "prettier:check": "prettier --check \"**/*.{ts,scss,css}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,scss,css}\"",
    "lint": "eslint",
    "stylelint": "stylelint '**/*.{css,scss}'",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@vitest/coverage-v8": "^3.1.2",
    "c8": "^10.1.3",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.1",
    "postcss-scss": "^4.0.9",
    "prettier": "3.5.3",
    "sass": "^1.87.0",
    "stylelint": "^16.19.1",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-scss": "^6.12.0",
    "stylelint-selector-bem-pattern": "^4.0.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.1",
    "vitest": "^3.1.2"
  }
}
